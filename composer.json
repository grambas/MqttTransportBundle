{
    "name": "kl3sk/mqtt-transport-bundle",
    "description": "Provide a MQTT transport for symfony messenger",
    "minimum-stability": "dev",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "kl3sk",
            "email": "klesk44@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "php-mqtt/client": "^1.8",
        "symfony/serializer": "6.2.*",
        "symfony/messenger": "6.2.*",
        "symfony/options-resolver": "6.2.*"
    },
    "require-dev": {
        "symfony/monolog-bundle": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kl3sk\\MqttTransportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kl3sk\\MqttTransportBundle\\": "tests/"
        }
    }
}